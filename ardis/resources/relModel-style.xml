<mxStylesheet>
	
    <add as="defaultVertex" extend="defaultVertex"> 
        <add as="strokeColor" value="black"/>
        <add as="fillColor" value="white"/>
        <add as="fontSize" value="11"/>
        <add as="fontColor" value="black"/>
        <add as="fontFamily" value="Arial"/>
    </add>
    
    <add as="table" extends="defaultVertex">
        <add as="shape" value="swimlane"/>
    </add>
    
    <add as="column" extends="defaultVertex">
        <add as="shape" value="rectangle"/>
    </add>
    
    <add as="cardinality" extends="defaultEdge" >
        <!--
        <add as="verticalLabelPosition" value="top"/>
        <add as="labelPosition" value="right"/>
        <add as="verticalAlign" value="bottom"/>
        -->
        <add as="align" value="right"/>
        <add as="verticalLabelPosition" value="left" />
        <add as="verticalAlign" value="bottom" />
    </add>
    
    
    <add as="defaultEdge">
        <add as="shape" value="connector" />
        <add as="edgeStyle" value="topToBottomEdgeStyle"/>
        <add as="endArrow" value="none" />
        <add as="strokeColor" value="black"/>
    </add>
   
	
</mxStylesheet>
